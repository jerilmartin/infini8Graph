# Server Configuration
PORT=3001
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Meta/Instagram OAuth Configuration
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_REDIRECT_URI=https://your-cloudflare-tunnel-url/api/auth/callback

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_min_32_chars
JWT_EXPIRES_IN=7d

# Encryption Key (for encrypting Instagram tokens)
ENCRYPTION_KEY=your_32_character_encryption_key

# Frontend URL (for CORS and redirects)
FRONTEND_URL=http://localhost:3000

# Cloudflare Tunnel URL (used during development)
CLOUDFLARE_TUNNEL_URL=https://your-tunnel-url.trycloudflare.com
